{
  "name": "mattpatterson94/mpwp-settings",
  "description": "A basic settings plugin for Wordpress",
  "homepage": "https://github.com/mattpatterson94/mpwp-settings",
  "license": "GPL-2.0+",
  "keywords": [
    "wordpress"
  ],
  "authors": [
    {
      "name": "Matt Patterson",
      "homepage": "https://mattdoesdev.com/"
    }
  ],
  "support": {
      "issues": "https://github.com/mattpatterson94/mpwp-settings/issues",
      "source": "https://github.com/mattpatterson94/mpwp-settings"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": ["wpackagist-plugin/*"]
    }
  ],
  "extra": {
    "installer-paths": {
      "{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "type": "wordpress-plugin",
  "require": {
    "composer/installers": "~1.0",
    "wpackagist-plugin/advanced-custom-fields": "5.10.2"
   }
}
